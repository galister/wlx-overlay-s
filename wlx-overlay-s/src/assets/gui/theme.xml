<layout>
  <theme>
    <var key="color_text" value="#FFFFFF" />
    <var key="color_text_translucent" value="#FFFFFF40" />

    <var key="color_accent" value="#008cff" />
    <var key="color_accent_translucent" value="#008cff40" />
